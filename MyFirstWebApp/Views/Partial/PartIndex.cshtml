@model MyFirstWebApp.Models.PersonPartialVM
@{
    ViewBag.Title = "PartIndex";
}

<h2>Partial Views Index</h2>

<div class="row">
    <div class="col-md-6">
        @foreach (var item in Model.persons)
        {
            Html.RenderAction("PartPerson", new { id = item.Id });
        }
    </div>
</div>